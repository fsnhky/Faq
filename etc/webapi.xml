<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/feedback/useful/:questionId/delta/:delta" method="GET">
        <service class="Nans\Faq\Api\FeedbackInterface" method="changeUseful"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/feedback/useless/:questionId/delta/:delta" method="GET">
        <service class="Nans\Faq\Api\FeedbackInterface" method="changeUseless"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>